package Array_2D;

import java.util.Scanner;

public class Q1 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		
		int ar[][]=new int[2][2];
		int i,j;
		
		System.out.println("Enter the Array Elements:-");
		for(i=0;i<ar.length;i++)
		{
			for(j=0;j<ar.length;j++)
			{
				ar[i][j]=sc.nextInt();
			}
		}
		
		for(i=0;i<ar.length;i++)
		{
			for(j=0;j<ar.length;j++)
			{
				System.out.print(ar[i][j]);
			}
			System.out.println();
		}
	}
}
