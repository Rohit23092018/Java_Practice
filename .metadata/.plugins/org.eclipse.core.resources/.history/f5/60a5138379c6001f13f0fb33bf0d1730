package Array_sec_c;

public class Qxiv 
{
	int factorial(int rem)
	{
		int fact=1;
		for(int i=rem;i>0;i--)
		{
			fact=fact*i;
		}
		return fact;
	}
	public static void main(String[] args) 
	{
		int ar[]= {145,6,37,131,153,56,33,1634,2};
		int i,temp,strong;
		for(i=0;i<ar.length;i++)
		{
			temp=ar[i];
			while(temp!=0)
			{
				rem=temp%10;
				fact=factorial(rem);
			}
		}
	}
}
