package Array_sec_f;

public class Qd 
{
	public static void main(String[] args) 
	{
		int ar[]= {49,57,78,12,36,18,89};
		int i,temp=ar[0];
		
		int ar1[]=new int[7];
		
		for(i=0;i<ar.length;i++)
		{
			if(ar[i]<temp)
			{
				temp=ar[i];
			}
			ar1[i]=temp;
		}
		for(i=0;i<ar1.length;i++)
		{
			System.out.print(ar1[i]+" ");
		}
	}
}
