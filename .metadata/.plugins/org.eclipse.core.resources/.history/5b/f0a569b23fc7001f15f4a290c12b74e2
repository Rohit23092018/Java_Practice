package Array_sec_e;

public class Qa 
{
	public static void main(String[] args) 
	{
		int ar[]= {78,77,78,17,71,17,78};
		int i,j,k,count;
		
		for(i=0;i<ar.length;i++)
		{
			count=0;
			for(j=i+1;j>=0;j--)
			{
				if(ar[i]==ar[j])
				{
					break;
				}
				else
				{
					for(k=0;k<ar.length;k++)
					{
						if(ar[i]==ar[k])
						{
							count++;
						}
					}
				}
			}
			if(count>1)
			{
				System.out.print(ar[i]+" ");
			}
		}
	}
}
