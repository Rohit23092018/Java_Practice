package Array_sec_f;

public class Qf 
{
	public static void main(String[] args) 
	{
		int ar[]= {1,2,3,4,5,6,7,8,9,0};
		int ar1[]=new int[ar.length];
		int i;
		
		int left=0;
		int right=ar.length-1;
		
		for(i=0;i<ar.length;i++)
		{
			if(ar[i]%2==0)
			{
				ar1[left++]=ar[i];
			}
			else
			{
				ar1[right--]=ar[i];
			}
		}
		for(i=0;i<ar1.length;i++)
		{
			System.out.print(ar1[i]+" ");
		}
  	}
}
