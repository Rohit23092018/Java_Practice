package String_sec_2;

public class Q26 
{
	public static void main(String[] args) 
	{
		String str="This is java exam code";
		int i,j,count=0;
		
		for(i=0;i<str.length();i++)
		{
			for(j=0;j<str.length();j++)
			{
				if(str.charAt(j)==' ')
				{
					break;
				}
			}
			count++;
		}
		System.out.println(count+" words are present in the given String.");
	}
}
