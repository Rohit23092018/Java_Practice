package Array_sec_c;

import java.math.BigInteger;

public class Qxiii 
{
	public static void main(String[] args) 
	{
		int ar[]= {6,131,37,145,153,56,33,1634};
		int i,j;
		BigInteger fact;
		
		for(i=0;i<ar.length;i++)
		{
			fact=1;
			for(j=ar[i];j>0;j--)
			{
				fact=fact*j;
			}
			System.out.println("Factorail of a number "+ar[i]+" is "+fact);
		}
	}
}
