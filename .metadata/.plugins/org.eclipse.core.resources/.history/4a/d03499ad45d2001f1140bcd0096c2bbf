package String_sec_2;

public class Q9 
{
	public static void main(String[] args) 
	{
		String s="Sathya";
		String str="";
		int i;
		
		for(i=0;i<s.length();i++)
		{
			if(s.charAt(0)>=65 && s.charAt(0)<=90)
			{
				str=str+s.charAt(0)+32;
			}
			str=str+s.charAt(i);
		}
		System.out.println(str);
	}
}
